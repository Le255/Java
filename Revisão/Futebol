import java.util.*;
public class Futebol {
public static void main(String[] args) {
	Scanner ler= new Scanner (System.in);
	int anoNas, id;
	System.out.println("Digite o ano de nascimento:");
	anoNas=ler.nextInt();
	id=2024-anoNas;
	
	if (id==6) {
		System.out.println("Você participará da categoria dente de leite");
		
	}else if (id<=10) {
		System.out.println("Você participará da categoria Infantil 1");
		
	}else if (id<=14) {
		System.out.println("Você participará da categoria Infantil 2");
		
	}else if (id<=16) {
		System.out.println("Você participará da categoria Juvenil 1");
		
	}else if(id<=17) {
		System.out.println("Você participará da categoria Juvenil 2");
		
	}else if (id<=18) {
		System.out.println("Você não poderá participar do campeonato");
	}
	
}
}
